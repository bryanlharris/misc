"filetype plugin on
set mouse=a
set smarttab
set autoindent
set smartindent
set nocompatible
set background=dark
set tabstop=4
set shiftwidth=4
set expandtab
set nowrap
set backspace=indent,eol,start
set nobackup
set nohlsearch
set ic
set guifont=Courier\ 16
syntax on
let &guicursor = &guicursor . ",a:blinkon0"
